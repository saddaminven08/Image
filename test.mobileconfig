            <string>Facebook</string>
            <key>URL</key>
            <string>fb://</string>
            <key>FullScreen</key>
            <true/>
            <key>Precomposed</key>
            <true/>
            <key>IconURL</key>
            <string>https://raw.githubusercontent.com/saddaminven08/Image/main/fb_icon.png</string>
            <key>IsRemovable</key>
            <true/>
